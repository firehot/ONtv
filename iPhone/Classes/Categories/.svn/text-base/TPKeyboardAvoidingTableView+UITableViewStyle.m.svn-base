#import "TPKeyboardAvoidingTableView+UITableViewStyle.h"

@interface TPKeyboardAvoidingTableView (UITableViewStyle_Private)

- (void)setup;

@end


@implementation TPKeyboardAvoidingTableView (UITableViewStyle)

- (id)initWithFrame:(CGRect)frame style:(UITableViewStyle)style
{
    self = [super initWithFrame:frame style:style];
    if (self) {
        [self setup];
    }
    return self;
}

@end
